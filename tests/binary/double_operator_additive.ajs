// additive
//
use extended_operators;

extern const x: Double;
extern const y: Double;

extern var r_identity: Double;
extern var r_negate: Double;

extern var r_add: Double;
extern var r_add_imm8: Double;
extern var r_add_imm32: Double;
extern var r_add_imm64: Double;

extern var r_subtract: Double;
extern var r_subtract_imm8: Double;
extern var r_subtract_imm32: Double;
extern var r_subtract_imm64: Double;

r_identity := +x;
r_negate := -x;

r_add_imm8 := x + 7.3;
r_add_imm32 := x + 128.01;
r_add_imm64 := x + 443258581523.45;

r_subtract := x - y;
r_subtract_imm8 := x - 7.03;
r_subtract_imm32 := x - 128.32;
r_subtract_imm64 := x - 443258581523.1;

// last returns the (result)
r_add := x + y;
